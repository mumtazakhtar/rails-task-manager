<%= form_with model: task do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :details %>
  <%= f.text_field :details %>
  <%= f.label :Completed%>
  <%= f.label :Yes%>
  <%= f.radio_button :completed, "true"%>
  <%= f.label :No%>
  <%= f.radio_button :completed, "false"%>
  <%= f.submit %>
<% end %>
